{
  "redirects": [
    { "source": "/index.html", "destination": "/", "permanent": true },
    { "source": "/sitemap.xml/", "destination": "/sitemap.xml", "permanent": true },

    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "fantasyblitz.de" }],
      "destination": "https://www.fantasyblitz.de/:path*",
      "permanent": true
    }
  ]
}
